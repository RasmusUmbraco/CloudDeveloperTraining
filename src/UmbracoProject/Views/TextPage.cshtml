@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<TextPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Extensions
@using Umbraco.Forms.Web;
@using Umbraco.Forms.Web.Extensions;   
@{
    Layout = "basicNav.cshtml";
                        
}
<section id="single-content" class="bg-white">
    <div class="container">
        <div class="section-content blog-content">
            <div class="row">
                <div class="col-md-2 mt-4 align-self-start">
                    <ul class="side-nav">
                        @await Html.PartialAsync("SideNav")
                    </ul>
                </div>
                <div class="col-md-8 mt-4">
                    @{
                        if (Model.HasValue("bodyText")){
                            <p>@(Model.Value("bodyText"))</p>
                        }
                    }
                </div>

            </div>
        </div>
    </div>
</section>